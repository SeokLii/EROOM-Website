                       <td style="width:10%"><b><%=data.code%></b></td> <!--hover 밑줄 없애기 - css 정리 필요-->
                       <td id="result<%=i%>">-</td>
                       <td><input style="width:80%" type="text" name="Borrower" id="Borrower"></td>
                       <td><input style="width:80%" type="text" name="studentID" id="studentID"></td>
