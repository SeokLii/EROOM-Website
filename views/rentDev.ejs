                       <td><input style="width: 20px;	height: 20px;" type="checkbox" name="return" value="<%=i%>" checked="checked" disabled="disabled"></td>
                   </tr>
                   <%}
                   else{
                     var standard_date = new Date();
                     var diff = standard_date - data.rentaldate/60000;
                     var gap = (standard_date.getTime() - data.rentaldate.getTime())/60000;
                     var hour = parseInt(gap/60);
                     var minute = parseInt(gap%60);
                     var diff_day = parseInt((71-hour)/24);
                     var diff_hour = (71-hour)%24;
                     var diff_minute = 60 - minute;

                     hour > 71
                     %>
                     <tr style ="text-align:center">
                         <td class="name<%=i%>"><b><%=data.name%></b></td>
                         <td><b><%=data.code%></b></td> <!--hover 밑줄 없애기 - css 정리 필요-->
                         <td id="result<%=i%>"><%=diff_day%>일 <%=diff_hour%>시간 <%=diff_minute%>분</td>
                         <td><%=data.Borrower%></td>
                         <td><%=data.studentID%></td>
                         <td><input id="rent<%=i%>" style="width: 20px;	height: 20px;" type="checkbox" name="return" value="<%=i%>" checked="checked" disabled="disabled"></td>
